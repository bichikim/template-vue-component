{
  "name": "template-vue-component",
  "version": "1.0.0",
  "author": "katashin",
  "private": true,
  "description": "Boilerplate for standalone Vue.js components",
  "keywords": [
    "component",
    "Vue.js"
  ],
  "license": "MIT",
  "main": "dist/template-vue-component.cjs.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/template-vue-component",
  "bugs": "https://github.com/ktsn/template-vue-component/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/template-vue-component.git"
  },
  "scripts": {
    "prepublishOnly": "npm run release",
    "clean": "rm -rf dist",
    "play": "poi --open --config scripts/play.config.js",
    "build": "bili",
    "build:play": "poi build --config scripts/play.config.js",
    "lint": "eslint --ext js,ts,vue src test scripts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "test:dev": "jest --watch",
    "release": "npm run test && npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "bili": "^3.1.2",
    "eslint": "^4.0.0",
    "eslint-config-ktsn": "^1.0.2",
    "eslint-plugin-html": "^3.0.0",
    "jest": "^23.4.1",
    "poi": "^8.0.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-vue": "^4.3.1",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2",
    "vue": "^2.3.4",
    "vue-jest": "^2.6.0",
    "vue-play": "^3.2.1"
  }
}
